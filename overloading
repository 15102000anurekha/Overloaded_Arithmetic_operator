#include<iostream>
#include<conio.h>
#include<math.h>

using namespace std;
class FLOAT
{
	float a;
	public:
		FLOAT(float a)
		{
			this->a=a;
		}
		float operator+(FLOAT F)
		{
			return(a+F.a);
		}
		float operator-(FLOAT F)
		{
			return(a-F.a);
		}
		float operator*(FLOAT F)
		{
			return(a*F.a);
		}
		float operator/(FLOAT F)
		{
			return(a/F.a);
		}
		  		
};
int main()
{
	FLOAT F1(34.12),F2(98.92);
	cout<<"\n\n Overloading Operator + : "<<f1+f2;
	cout<<"\n\n Overloading Operator - : "<<f1-f2;
	cout<<"\n\n Overloading Operator * : "<<f1*f2;
	cout<<"\n\n Overloading Operator / : "<<f1/f2;
return 0;
}
