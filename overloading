#include<iostream>
#include<conio.h>
using namespace std;
class FLOAT
{
	float b;
	public:
		FLOAT(float b)
		{
			this->b=b;
		}
		float operator+(FLOAT F)
		{
			return(b+F.b);
		}
		float operator-(FLOAT F)
		{
			return(b-F.b);
		}
		float operator*(FLOAT F)
		{
			return(b*F.b);
		}
		float operator/(FLOAT F)
		{
			return(b/F.b);
		}
		  		
};
int main()
{
	FLOAT f1(23.12),f2(20.12);
	cout<<"\n\n Operator + : "<<f1+f2;
	cout<<"\n\n Operator - : "<<f1-f2;
	cout<<"\n\n Operator * : "<<f1*f2;
	cout<<"\n\n Operator / : "<<f1/f2;
return 0;
}
